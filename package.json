{
  "name": "rubin-client",
  "version": "1.2.7",
  "private": true,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/lsst-epo/rubin-obs-client"
  },
  "scripts": {
    "dev": "next dev",
    "static": "npm-run-all static:*",
    "static:build": "next build --debug",
    "static:export": "next export",
    "start": "next start -p 8080",
    "bs": "next build && next start -p 3000",
    "bs:inspect": "next build && node --inspect node_modules/next/dist/bin/next start -p 3000",
    "prepare": "husky install",
    "precommit": "lint-staged",
    "commitlint": "commitlint --edit",
    "lint": "npm-run-all lint:*",
    "lint:js": "prettier --check \"**/*.{js,jsx,ts,tsx}\" & eslint -c .eslintrc.cjs \"**/*.{js,jsx}\"",
    "lint:styles": "stylelint --config .stylelintrc.cjs \"**/*.{css,scss}\" \"**/styles.{js,ts}\"",
    "fix": "npm-run-all fix:*",
    "fix:js": "prettier --write \"**/*.{js,jsx,ts,tsx}\" && eslint -c .eslintrc.cjs \"**/*.{js,jsx}\" --fix",
    "fix:styles": "stylelint --config .stylelintrc.cjs \"**/*.{css,scss}\" \"**/styles.{js,ts}\" --fix",
    "test": "npm-run-all test:*",
    "test:unit": "cypress run --component --headless",
    "test:e2e": "cypress run --e2e --headless",
    "cypress": "CYPRESS_BASE_URL=http://localhost:3000 cypress open",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "proxy": "env-cmd -x lcp --proxyUrl \\$EFD_URL_PROXY",
    "codegen:noirlab": "openapi-ts",
    "codegen:gql": "graphql-codegen --config codegen.ts"
  },
  "babel": {
    "presets": [
      "next/babel"
    ],
    "plugins": [
      [
        "babel-plugin-styled-components",
        {
          "ssr": true,
          "displayName": true
        }
      ]
    ]
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ],
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "yarn fix:js",
    "*.{css,scss}": "yarn fix:styles",
    "**/styles.{js,ts}": "yarn fix:styles"
  },
  "dependencies": {
    "@castiron/style-mixins": "^1.0.6",
    "@headlessui/react": "^2.2.1",
    "@hey-api/client-next": "^0.2.3",
    "@influxdata/influxdb-client": "^1.33.2",
    "@popperjs/core": "^2.11.5",
    "@react-oauth/google": "^0.12.1",
    "@rubin-epo/epo-react-lib": "^2.9.5",
    "@t3-oss/env-nextjs": "^0.12.0",
    "@urql/core": "^5.1.1",
    "@urql/exchange-retry": "^1.3.1",
    "@urql/next": "^1.1.5",
    "add": "^2.0.6",
    "clsx": "^2.1.1",
    "convert": "^4.13.0",
    "fast-xml-parser": "^4.5.0",
    "feed": "^4.2.2",
    "focus-trap": "^7.0.0",
    "focus-visible": "^5.1.0",
    "google-auth-library": "^9.6.3",
    "graphql": "^16.5.0",
    "graphql-request": "^5.0.0",
    "hoist-non-react-statics": "^3.3.2",
    "i18next": "23",
    "i18next-browser-languagedetector": "^8.0.0",
    "i18next-resources-to-backend": "^1.2.1",
    "json-to-scss": "^1.6.2",
    "jwt-decode": "^3.1.2",
    "next": "^16.1.5",
    "next-build-id": "^3.0.0",
    "next-intl": "^4.1.0",
    "npm-run-all": "^4.1.5",
    "postcss-utopia": "^1.1.0",
    "prom-client": "^15.1.3",
    "react": "^18.3.1",
    "react-compare-slider": "^3.1.0",
    "react-dom": "^18.3.1",
    "react-headroom": "^3.2.1",
    "react-hook-form": "^7.33.1",
    "react-i18next": "13",
    "react-icons": "^5.5.0",
    "react-is": "^18.2.0",
    "react-loading-skeleton": "^3.5.0",
    "react-player": "^2.12.0",
    "react-popper": "^2.3.0",
    "sanitize-html": "^2.15.0",
    "schema-dts": "^1.1.2",
    "server-only": "^0.0.1",
    "sharp": "^0.33.5",
    "styled-components": "^6.1.16",
    "swr": "^1.3.0",
    "tinyduration": "^3.4.1",
    "tz-lookup": "^6.1.25",
    "urql": "^4.1.0",
    "use-resize-observer": "^9.0.2",
    "usehooks-ts": "^3.1.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@csstools/postcss-global-data": "^3.0.0",
    "@graphql-codegen/cli": "^5.0.3",
    "@graphql-codegen/client-preset": "^4.5.1",
    "@hey-api/openapi-ts": "0.64.10",
    "@next/env": "^15.1.3",
    "@rushstack/eslint-patch": "^1.1.4",
    "@storybook/addon-a11y": "^8.6.12",
    "@storybook/addon-essentials": "^8.6.12",
    "@storybook/addon-interactions": "^8.6.12",
    "@storybook/addon-links": "^8.6.12",
    "@storybook/blocks": "^8.6.12",
    "@storybook/nextjs": "^8.6.12",
    "@storybook/react": "^8.6.12",
    "@types/node": "^22.6.1",
    "@types/react": "^18.3.8",
    "@types/react-dom": "^18",
    "@types/react-headroom": "^3.2.3",
    "@types/sanitize-html": "^2.15.0",
    "babel-plugin-styled-components": "^2.1.4",
    "browserslist": "^4.21.2",
    "cypress": "13",
    "env-cmd": "^10.1.0",
    "eslint": "8.57",
    "eslint-config-next": "^14.2.13",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-cypress": "^4.2.0",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-storybook": "^0.12.0",
    "eslint-plugin-unused-imports": "3",
    "husky": "^8.0.3",
    "jiti": "^2.4.2",
    "jsconfig-paths-jest-mapper": "^1.0.0",
    "lint-staged": "^13.0.3",
    "local-cors-proxy": "^1.1.0",
    "postcss": "^8.4.14",
    "postcss-custom-media": "^11.0.5",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^7.7.2",
    "postcss-styled-syntax": "^0.7.0",
    "prettier": "^2.7.1",
    "sass": "^1.53.0",
    "storybook": "^8.6.12",
    "storybook-react-i18next": "^3.2.1",
    "stylelint": "^16.12.0",
    "stylelint-config-css-modules": "^4.4.0",
    "stylelint-config-recess-order": "^5.1.1",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-standard-scss": "^14.0.0",
    "typescript": "^5.6.2",
    "typescript-plugin-css-modules": "^5.1.0"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
