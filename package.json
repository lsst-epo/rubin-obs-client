{
  "name": "rubin-client",
  "version": "0.1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/castiron/rubin"
  },
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:next": "next dev",
    "static": "npm-run-all static:*",
    "static:build": "next build --debug",
    "static:export": "next export",
    "start": "next start -p 8080",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint -c .eslintrc.js \"**/*.{js,jsx}\"",
    "lint:scss": "stylelint --config .stylelintrc.js \"**/*.scss\"",
    "fix": "npm-run-all fix:js fix:scss",
    "fix:js": "eslint -c .eslintrc.js \"**/*.{js,jsx}\" --fix",
    "fix:scss": "stylelint --config .stylelintrc.js \"**/*.scss\" --fix",
    "fix:staged": "lint-staged",
    "test": "npm-run-all test:*",
    "test:e2e": "testcafe"
  },
  "babel": {
    "presets": [
      "next/babel"
    ],
    "plugins": [
      [
        "styled-components",
        {
          "ssr": true,
          "displayName": true
        }
      ]
    ]
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ],
  "lint-staged": {
    "*.{js,jsx}": "yarn fix:js",
    "*.scss": "yarn fix:scss"
  },
  "dependencies": {
    "@castiron/components-breadcrumbs": "^1.1.0",
    "@castiron/style-mixins": "^1.0.6",
    "@headlessui/react": "^1.6.6",
    "@popperjs/core": "^2.11.5",
    "add": "^2.0.6",
    "classnames": "^2.3.1",
    "flickity": "^3.0.0",
    "focus-trap": "^6.9.4",
    "focus-visible": "^5.1.0",
    "graphql": "^16.5.0",
    "graphql-request": "^4.3.0",
    "hoist-non-react-statics": "^3.3.2",
    "i18next": "^21.8.14",
    "json-to-scss": "^1.6.2",
    "jwt-decode": "^3.1.2",
    "next": "^12.2.0",
    "next-build-id": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-google-login": "^5.2.2",
    "react-hook-form": "^7.33.1",
    "react-i18next": "^11.18.1",
    "react-is": "^18.2.0",
    "react-player": "^2.10.1",
    "react-popper": "^2.3.0",
    "react-share": "^4.4.0",
    "react-uid": "^2.3.2",
    "striptags": "^3.2.0",
    "styled-components": "^5.3.5",
    "swr": "^1.3.0",
    "use-resize-observer": "^9.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/eslint-parser": "^7.18.2",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-react-jsx": "^7.18.6",
    "@rushstack/eslint-patch": "^1.1.4",
    "babel-plugin-styled-components": "^2.0.7",
    "browserslist": "^4.21.2",
    "eslint": "8.19.0",
    "eslint-config-next": "^12.2.2",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-testcafe": "^0.2.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.14",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^7.7.2",
    "prettier": "^2.7.1",
    "sass": "^1.53.0",
    "stylelint": "^14.9.1",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard-scss": "^4.0.0",
    "testcafe": "^1.20.0"
  }
}
