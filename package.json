{
  "name": "rubin-client",
  "version": "1.2.2",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/lsst-epo/rubin-obs-client"
  },
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:next": "next dev",
    "static": "npm-run-all static:*",
    "static:build": "next build --debug",
    "static:export": "next export",
    "start": "next start -p 8080",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint \"**/*.{js,jsx}\"",
    "lint:scss": "stylelint --config .stylelintrc.js \"**/*.scss\"",
    "fix": "npm-run-all fix:js fix:scss",
    "fix:js": "eslint \"**/*.{js,jsx}\" --fix",
    "fix:scss": "stylelint --config .stylelintrc.js \"**/*.scss\" --fix",
    "fix:staged": "lint-staged",
    "test": "npm-run-all test:*",
    "test:int": "testcafe --config-file int.testcaferc.js",
    "test-int-local": "testcafe --config-file int.testcaferc.js --hostname localhost",
    "test:unit": "jest --config jest.config.js"
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ],
  "lint-staged": {
    "*.{js,jsx}": "yarn fix:js",
    "*.scss": "yarn fix:scss"
  },
  "dependencies": {
    "@castiron/components-breadcrumbs": "^1.1.0",
    "@castiron/style-mixins": "^1.0.6",
    "@headlessui/react": "^1.6.6",
    "@popperjs/core": "^2.11.5",
    "@rubin-epo/epo-react-lib": "^1.0.12",
    "add": "^2.0.6",
    "classnames": "^2.3.1",
    "feed": "^4.2.2",
    "flickity": "^3.0.0",
    "focus-trap": "^7.0.0",
    "focus-visible": "^5.1.0",
    "graphql": "^16.5.0",
    "graphql-request": "^5.0.0",
    "hoist-non-react-statics": "^3.3.2",
    "i18next": "^22.0.4",
    "json-to-scss": "^1.6.2",
    "jwt-decode": "^3.1.2",
    "next": "^13.0.1",
    "next-build-id": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-google-login": "^5.2.2",
    "react-hook-form": "^7.33.1",
    "react-i18next": "^12.0.0",
    "react-is": "^18.2.0",
    "react-popper": "^2.3.0",
    "react-share": "^4.4.0",
    "react-uid": "^2.3.2",
    "striptags": "^3.2.0",
    "styled-components": "^5.3.5",
    "swr": "^1.3.0",
    "use-resize-observer": "^9.0.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.18.2",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-react-jsx": "^7.18.6",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.3.0",
    "browserslist": "^4.21.2",
    "eslint": "^8.26.0",
    "eslint-config-next": "^13.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.3",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testcafe": "^0.2.1",
    "eslint-plugin-testing-library": "^5.6.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^29.2.2",
    "jest-environment-jsdom": "^29.2.2",
    "jsconfig-paths-jest-mapper": "^1.0.0",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.14",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^7.7.2",
    "prettier": "^2.7.1",
    "sass": "^1.53.0",
    "stylelint": "^14.9.1",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard-scss": "^6.0.0",
    "testcafe": "^2.2.0"
  }
}
