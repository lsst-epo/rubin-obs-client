{
  "name": "rubin-client",
  "version": "1.2.7",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/lsst-epo/rubin-obs-client"
  },
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:next": "next dev",
    "static": "npm-run-all static:*",
    "static:build": "next build --debug",
    "static:export": "next export",
    "start": "next start -p 8080",
    "bs": "next build && next start -p 3000",
    "prepare": "husky install",
    "precommit": "lint-staged",
    "lint": "npm-run-all lint:*",
    "lint:js": "prettier --check \"**/*.{js,jsx}\" & eslint -c .eslintrc.js \"**/*.{js,jsx}\"",
    "lint:scss": "stylelint --config scss.stylelintrc.js \"**/*.scss\"",
    "lint:styled": "stylelint --config styled.stylelintrc.js \"**/styles.js\"",
    "fix": "npm-run-all fix:js fix:scss fix:styled",
    "fix:js": "prettier --write \"**/*.{js,jsx}\" && eslint -c .eslintrc.js \"**/*.{js,jsx}\" --fix",
    "fix:scss": "stylelint --config scss.stylelintrc.js \"**/*.scss\" --fix",
    "fix:styled": "stylelint --config styled.stylelintrc.js \"**/styles.js\" --fix",
    "test": "npm-run-all test:*",
    "test:unit": "cypress run --component --headless",
    "test:e2e": "cypress run --e2e --headless",
    "cypress": "CYPRESS_BASE_URL=http://localhost:3000 cypress open",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ],
  "lint-staged": {
    "*.{js,jsx}": "yarn fix:js",
    "*.scss": "yarn fix:scss",
    "**/styles.js": "yarn fix:styled"
  },
  "dependencies": {
    "@castiron/components-breadcrumbs": "^1.1.0",
    "@castiron/style-mixins": "^1.0.6",
    "@headlessui/react": "^1.6.6",
    "@popperjs/core": "^2.11.5",
    "@rubin-epo/epo-react-lib": "^1.2.9",
    "add": "^2.0.6",
    "classnames": "^2.3.1",
    "convert": "^4.13.0",
    "feed": "^4.2.2",
    "focus-trap": "^7.0.0",
    "focus-visible": "^5.1.0",
    "graphql": "^16.5.0",
    "graphql-request": "^5.0.0",
    "hoist-non-react-statics": "^3.3.2",
    "i18next": "^22.0.4",
    "json-to-scss": "^1.6.2",
    "jwt-decode": "^3.1.2",
    "next": "^13.0.1",
    "next-build-id": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-google-login": "^5.2.2",
    "react-hook-form": "^7.33.1",
    "react-i18next": "^12.0.0",
    "react-is": "^18.2.0",
    "react-popper": "^2.3.0",
    "react-uid": "^2.3.2",
    "striptags": "^3.2.0",
    "styled-components": "5.3.11",
    "swr": "^1.3.0",
    "use-resize-observer": "^9.0.2"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@storybook/addon-a11y": "^7.0.8",
    "@storybook/addon-essentials": "^7.0.6",
    "@storybook/addon-interactions": "^7.0.6",
    "@storybook/addon-links": "^7.0.6",
    "@storybook/blocks": "^7.0.6",
    "@storybook/nextjs": "^7.0.6",
    "@storybook/react": "^7.0.6",
    "browserslist": "^4.21.2",
    "cypress": "^12.15.0",
    "eslint": "^8.26.0",
    "eslint-config-next": "^13.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-storybook": "^0.6.11",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-http-backend": "^2.2.0",
    "jsconfig-paths-jest-mapper": "^1.0.0",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.14",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^7.7.2",
    "postcss-styled-syntax": "^0.4.0",
    "prettier": "^2.7.1",
    "sass": "^1.53.0",
    "storybook": "^7.0.6",
    "storybook-react-i18next": "^2.0.1",
    "stylelint": "^15.10.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recess-order": "^4.2.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-config-standard-scss": "^9.0.0",
    "tsconfig-paths-webpack-plugin": "^4.0.1",
    "typescript": "5.0.4"
  }
}
