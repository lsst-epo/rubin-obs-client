{
  "name": "rubin-client",
  "version": "1.2.7",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/lsst-epo/rubin-obs-client"
  },
  "scripts": {
    "dev": "next dev",
    "static": "npm-run-all static:*",
    "static:build": "next build --debug",
    "static:export": "next export",
    "start": "next start -p 8080",
    "bs": "next build && next start -p 3000",
    "prepare": "husky install",
    "precommit": "lint-staged",
    "lint": "npm-run-all lint:*",
    "lint:js": "prettier --check \"**/*.{js,jsx}\" & eslint -c .eslintrc.js \"**/*.{js,jsx}\"",
    "lint:styles": "stylelint --config scss.stylelintrc.js \"**/*.{css,scss}\" \"**/styles.{js,ts}\"",
    "fix": "npm-run-all fix:*",
    "fix:js": "prettier --write \"**/*.{js,jsx}\" && eslint -c .eslintrc.js \"**/*.{js,jsx}\" --fix",
    "fix:styles": "stylelint --config scss.stylelintrc.js \"**/*.{css,scss}\" \"**/styles.{js,ts}\" --fix",
    "test": "npm-run-all test:*",
    "test:unit": "cypress run --component --headless",
    "test:e2e": "cypress run --e2e --headless",
    "cypress": "CYPRESS_BASE_URL=http://localhost:3000 cypress open",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "proxy": "env-cmd -x lcp --proxyUrl \\$EFD_URL_PROXY",
    "openapi": "openapi-ts"
  },
  "babel": {
    "presets": [
      "next/babel"
    ],
    "plugins": [
      [
        "babel-plugin-styled-components",
        {
          "ssr": true,
          "displayName": true
        }
      ]
    ]
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ],
  "lint-staged": {
    "*.{js,jsx}": "yarn fix:js",
    "*.scss": "yarn fix:scss",
    "**/styles.js": "yarn fix:styled"
  },
  "dependencies": {
    "@castiron/components-breadcrumbs": "2.0.2",
    "@castiron/style-mixins": "^1.0.6",
    "@headlessui/react": "^1.6.6",
    "@hey-api/client-fetch": "^0.4.2",
    "@influxdata/influxdb-client": "^1.33.2",
    "@popperjs/core": "^2.11.5",
    "@react-oauth/google": "^0.12.1",
    "@rubin-epo/epo-react-lib": "^2.5.3",
    "@urql/core": "^5.0.6",
    "@urql/next": "^1.1.2",
    "add": "^2.0.6",
    "classnames": "^2.3.1",
    "convert": "^4.13.0",
    "fast-xml-parser": "^4.5.0",
    "feed": "^4.2.2",
    "focus-trap": "^7.0.0",
    "focus-visible": "^5.1.0",
    "google-auth-library": "^9.6.3",
    "graphql": "^16.5.0",
    "graphql-request": "^5.0.0",
    "hoist-non-react-statics": "^3.3.2",
    "i18next": "23",
    "i18next-browser-languagedetector": "^8.0.0",
    "i18next-resources-to-backend": "^1.2.1",
    "json-to-scss": "^1.6.2",
    "jwt-decode": "^3.1.2",
    "next": "^14.2.15",
    "next-build-id": "^3.0.0",
    "next-i18n-router": "^5.5.1",
    "npm-run-all": "^4.1.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-headroom": "^3.2.1",
    "react-hook-form": "^7.33.1",
    "react-i18next": "13",
    "react-is": "^18.2.0",
    "react-player": "^2.12.0",
    "react-popper": "^2.3.0",
    "sanitize-html": "^2.13.1",
    "schema-dts": "^1.1.2",
    "sharp": "^0.33.5",
    "striptags": "^3.2.0",
    "styled-components": "^6.1.1",
    "swr": "^1.3.0",
    "tz-lookup": "^6.1.25",
    "urql": "^4.1.0",
    "use-resize-observer": "^9.0.2"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "^0.53.11",
    "@next/env": "^15.1.3",
    "@rushstack/eslint-patch": "^1.1.4",
    "@storybook/addon-a11y": "^7.0.8",
    "@storybook/addon-essentials": "^7.0.6",
    "@storybook/addon-interactions": "^7.0.6",
    "@storybook/addon-links": "^7.0.6",
    "@storybook/blocks": "^7.0.6",
    "@storybook/nextjs": "^7.0.6",
    "@storybook/react": "^7.0.6",
    "@types/node": "^22.6.1",
    "@types/react": "^18.3.8",
    "@types/react-headroom": "^3.2.3",
    "@types/sanitize-html": "^2.13.0",
    "babel-plugin-styled-components": "^2.1.4",
    "browserslist": "^4.21.2",
    "cypress": "^12.15.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.26.0",
    "eslint-config-next": "^14.2.13",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-storybook": "^0.6.11",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "jsconfig-paths-jest-mapper": "^1.0.0",
    "lint-staged": "^13.0.3",
    "local-cors-proxy": "^1.1.0",
    "postcss": "^8.4.14",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^7.7.2",
    "postcss-styled-syntax": "^0.7.0",
    "prettier": "^2.7.1",
    "sass": "^1.53.0",
    "storybook": "^7.0.6",
    "storybook-react-i18next": "^2.0.1",
    "stylelint": "^16.12.0",
    "stylelint-config-css-modules": "^4.4.0",
    "stylelint-config-recess-order": "^5.1.1",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-standard-scss": "^14.0.0",
    "tsconfig-paths-webpack-plugin": "^4.0.1",
    "typescript": "^5.6.2",
    "typescript-plugin-css-modules": "^5.1.0"
  }
}
